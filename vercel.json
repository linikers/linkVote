{
    "rewrites": [
      { "source": "/api/:path*", "destination": "/api/:path*" }
    ],
    "builds": [
      { "src": "api/**/*.ts", "use": "@vercel/node" },
      { "src": "vite.config.ts", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ]
  }
  